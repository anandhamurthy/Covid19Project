<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.covid19project">
    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TweetsActivity"
            android:label="Tweets"/>
        <activity android:name=".OnlineDoctorsActivity"
            android:label="Online Doctors"/>
        <activity android:name=".HospitalAdmissionActivity"
            android:label="Hospital Admissions"/>
        <activity android:name=".HealthCareActivity" />
        <activity
            android:name=".OrphanageSupportActivity"
            android:label="Orphanage and Vulnerable Support" />
        <activity
            android:name=".ConfirmActivity"
            android:label="Confirm Your Details" />
        <activity
            android:name=".OrphanageSupport.FreeFoodActivity"
            android:label="Free Foods">
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />

            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OrphanageSupport.HomesActivity"
            android:label="Free Accommodations Homes">
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />

            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OrphanageSupport.DeliveryActivity"
            android:label="Free Delivery">
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />

            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VolunteersActivity"
            android:label="Volunteers and Donors">
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />

            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".CoronaActivity"
            android:label="TN Corona Status"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".Services.WebViewActivity"
            android:label="Web"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".DonateMaterialsActivity"
            android:label="Donate"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".DonateDrugActivity"
            android:label="Donate Drugs"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".DonateReliefActivity"
            android:label="Donate Relief Materials"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".Login.ProfileActivity"
            android:label="Profile"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".Login.VerifyActivity"
            android:label="Verify"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".Login.LoginActivity"
            android:label="Login"
            android:screenOrientation="unspecified" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".MedicalStoresActivity"
            android:label="@string/title_activity_maps" />
        <activity android:name=".FAQsActivity" />
        <activity
            android:name=".TollNumbersActivity"
            android:label="Toll Free Numbers"
            android:screenOrientation="sensor" />
        <activity
            android:name=".ImageDetailedActivity"
            android:label="Image Zoom"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".Login.SplashActivity"
            android:theme="@style/AppThemeDonate">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeTreamentActivity"
            android:label="Home Treatment"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".MyHealthStatusActivity"
            android:label="My Health Status Check"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".TestLabsActivity"
            android:label="Test Labs"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".DonateFundsActivity"
            android:label="Donate Funds"
            android:screenOrientation="unspecified" />
        <activity
            android:name=".CounsellingActivity"
            android:label="Counselling"
            android:screenOrientation="unspecified" />
    </application>

</manifest>